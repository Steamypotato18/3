        <script type="text/javascript">
        jQuery(document).ready(function($) {
            $('#button-loadmore').click(function(e) {
                var pageNumber = parseInt($(this).attr('data-page')),
                    cid = $(this).data('cid'),
                    total = $(this).data('total');

                $.ajax({
                    type: "POST",
                    dataType: "html",
                    url: "/wp-admin/admin-ajax.php",
                    data: {
                        action : 'more_comment_ajax',
                        pageNumber : pageNumber,
                        postId : cid,
                        total: total,
                    },
                    beforeSend: function() {
                        // setting a timeout
                        $(".sidebar-loader-comment").show();
                        $('.wrapper_button_load_more').hide();
                    },
                    success: function (data) {
                        pageNumber++;

                        $('#button-loadmore').attr('data-page',pageNumber);
                        $(data).insertBefore('#li-loadmore');
                        // console.log(data);
                        // count comment that display
                        $(".sidebar-loader-comment").hide();
                        $('.wrapper_button_load_more').show();
                        var countLi = $("#comments ol.comment-list li").filter(function(){
                            return $(this).data('parent') == 0
                        }).length;
                        if(countLi == total || countLi > total ){
                            // hide button
                            $('#li-loadmore').remove();
                        }

                        if ($('.newest_comment_studio').length > 0) {
                            $('.newest_comment_studio').each(function() {
                                var tem_id = $(this).attr('id');
                                if($('#'+ tem_id+ ':not(.newest_comment_studio)').length > 0) {
                                    $(this).remove();
                                    $('#'+ tem_id+ ':not(.newest_comment_studio)').addClass('newest_comment_studio newest_comment_studio_effect');
                                }
                            });
                             setTimeout(function () {
                                    $('.newest_comment_studio').removeClass('newest_comment_studio_effect');
                             }, 200);
                        }

                        load_thumbup_ajax_append();

                        setTimeout(function() {
                            vrp_load_style_child_comment();
                        }, 500);
                    },
                    error : function(error){ console.log(error) }

                });

                return false;

            });

        });

        </script>
        <script>
        jQuery(document).ready(function($) {
            if ($('#comments').length > 0) {
                if($('.box_comment_clone_top').length === 0) {
                    //clone box comment on top
                    var $form = $("#respond").clone().addClass('box_comment_clone_top').attr('id','response_comment_top');

                    $('form', $form).attr('id', 'commentform_top');
                    $('form textarea[name="comment"]', $form).attr('id', 'comment_top');
                    $('form #submit', $form).addClass('submit_form_clone btn_tertiary_newStyle');
                    $('form .vrp_btn_cancel_comment', $form).addClass('btn_tertiary_newStyle');

                    $form.insertAfter('#respond');
                }

                vrp_load_style_child_comment();

                if ($('.comment-reply-link-new').length > 0) {
                    $('.comment-reply-link-new').live('click touch', function (event) {
                        var $wrap = $(this).closest('.comment_root');

                        var id_root = $wrap.attr('id_root'), id_parent = $wrap.attr('id'), data_postid = $wrap.attr('data_postid');
                        $('.vrp_comment_form').removeClass('install');
                        // $("#respond").hide();
                        if(!$(this).closest('.comment_root').hasClass('hasCommentAfter')) {
                            $('.comment_root').removeClass('hasCommentAfter');
                            $wrap.addClass('hasCommentAfter');
                            $('.box_comment_clone').remove();
                            setTimeout(function () {
                                var $form = $("#respond").clone().addClass('box_comment_clone').attr('id','response_comment');

                                $("#comment_post_ID", $form).val(data_postid);
                                $("#comment_parent", $form).val(id_root);
                                $('#submit', $form).addClass('submit_form_clone');

                                $form.insertAfter('.comment_root_'+id_root);

                                //validate clone form
                                $('#submit', $form).click(function () {
                                    var $parent = $(this).closest('.vrp_comment_form');
                                    $('.comment-form .alert-comment', $parent).remove();
                                    var text_comment = $.trim($('.comment-form textarea', $parent).val());
                                    if (text_comment == '') {
                                        $('.comment-form textarea[name="comment"]', $parent).after('<p class="alert-comment">Please fill in the required field.</p>');
                                        return false;
                                    }
                                });

                                //auto resisize texarea
                                $('.vrp_comment_form textarea[name="comment"]').each(function () {
                                    this.setAttribute("rows", "1");
                                    this.setAttribute("style", "height:" + (this.scrollHeight) + "px;overflow-y:hidden;");
                                }).on("input", function () {
                                    this.style.height = "auto";
                                    this.style.height = (this.scrollHeight) + "px";
                                });

                                $('#comments').trigger('vrp-comment-form-added', [$('form', $form)]);
                            },100);
                        }

                        $('.vrp_comment_form.box_comment_clone').show();
                        $('.vrp_comment_form.box_comment_clone .form-submit').show();

                        setTimeout(function () {
                            $('.box_comment_clone textarea').focus();
                        },500);
                    });
                }

                if($('.vrp_comment_form .comment-form textarea').length > 0) {
                    $('.vrp_comment_form .comment-form textarea').live('click', function() {
                        if($(this).closest('.vrp_comment_form').hasClass('box_comment_clone_top')) {
                            $(this).closest('.comment-form').children('.form-submit').show();
                        }
                    });

                    $('.vrp_comment_form .vrp_btn_cancel_comment').live('click', function() {
                        if($(this).closest('.vrp_comment_form').hasClass('box_comment_clone_top')) {
                            $(this).closest('.comment-form').children('.form-submit').hide();
                            $(this).closest('.comment-form').children('.alert-comment').remove();
                        } else {
                            $(this).closest('.vrp_comment_form').hide();
                        }
                    });

                    $(document).on("click", function(event) {
                        if(!$(event.target).closest("#commentform_top").length){
                            $('#commentform_top .form-submit').hide();
                        }

                        if(!$(event.target).closest("#response_comment").length  && !$(event.target).hasClass('comment-reply-link-new')){
                            $('#response_comment .form-submit').hide();
                        } else {
                            $('#response_comment .form-submit').show();
                        }
                    });
                }


                //validate clone form
                $('.submit_form_clone').live('click', function () {
                    var $parent = $(this).closest('.vrp_comment_form');
                    $('.comment-form .alert-comment', $parent).remove();
                    var text_comment = $.trim($('.comment-form textarea', $parent).val());
                    if (text_comment == '') {
                        $('.comment-form textarea[name="comment"]', $parent).after('<p class="alert-comment">Please fill in the required field.</p>');
                        return false;
                    }
                });

                //auto resisize texarea
                $('.vrp_comment_form textarea[name="comment"]').each(function () {
                    this.setAttribute("rows", "1");
