                        }).wrap( "<span class='auto_wrap_blog'></span>" );
                }
            }
        });
    </script>
        <script type="text/javascript">
        (function( $ ) {
            if( getCookieCustom("is_notice_top") == "true" ) {
                $(".noticepopup_top").hide();
            } else {
                $(".noticepopup_top").show();
            }
            if (getCookieCustom("is_notice_top") == "true") {
                //Already notice and accepted
            } else {
                if (($(".noticebtn_ok").length > 0)) {
