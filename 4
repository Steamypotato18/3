                jQuery(document).ready(function($) {
                    $(document).on('lottieScriptLoaded', function () {
                        let list_menu_icon = {}, $icons = $('.menu-icon-animate');
                        if($icons.length > 0) {
                            $icons.each(function (index) {
                                var elm_icon = $(this).find('.icon_lotties').first();
                                if (elm_icon.length > 0) {
                                    var data_icon = {
                                        container: elm_icon[0],
                                        renderer: 'svg',
                                        loop: false,
                                        autoplay: false,
                                        path: elm_icon.data('json')
                                    }

                                    $(this).attr('iconIndex', index);
                                    list_menu_icon[index] = bodymovin.loadAnimation(data_icon);

                                    //auto play when current item
                                    if($(this).hasClass('current-menu-item')){
                                        list_menu_icon[index].setDirection(1);
                                        list_menu_icon[index].play();
                                    }
                                }
                            });
                            $(document).on({
                                'hover mouseenter touchstart touchmove': function () {
                                    var cindex = $(this).attr('iconIndex');
                                    if (cindex) {
                                        var thisIcon = list_menu_icon[cindex];
                                        thisIcon.setDirection(1);
                                        thisIcon.play();
                                    }
                                },
                                'mouseleave touchend touchcancel ': function () {
                                    var cindex = $(this).attr('iconIndex');
                                    if (cindex) {
                                        var thisIcon = list_menu_icon[cindex];
                                        thisIcon.setDirection(-1);
                                        thisIcon.stop();
                                    }
                                }
                            }, ".menu-icon-animate");
                        }
                    });
                });
            </script>
