            function banner_spot_rotation(count){
                var checkthis = Math.floor(Math.random() * 100);
                var item = 0;
                if(count == 2){
                    if(checkthis <= 50){
                        item = 0;
                    } else {
                        item = 1;
                    }
                } else if(count == 3){
                    if(checkthis <= 33){
                        item = 0;
                    } else if(checkthis > 33 && checkthis <= 66){
                        item = 1;
                    } else {
                        item = 2;
                    }
                } else {
                    if(count > 3){
                        item = Math.floor(Math.random() * count) ;
                    }
                }
                return item;

            }
