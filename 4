                    jQuery('.'+cookieName).remove();
                    jQuery('body').removeClass('have_custom_bar');
                    jQuery('.site-header').removeClass('have-custom-bar-site-header');
                    // set cookie or ?
                    //document.cookie = 'notshowBar' + "=" + "yes" + "; " + expireCookie + ";path=/";
                    // reset cookie
                    setCookieCustom(cookieName, 'yes',3600*24*3, "/");
                }

