                    //Function to add tracking Start
                    function handle_pagehide(e) {
                        let play_duration = 0;
                        if(timer) {
                            play_duration = timer.getTotalTimeValues().seconds;

                            if (timer.isRunning()) timer.pause();

                            if(!e || !e.vrp_keep_timer) timer.stop();
                        }


                        if(play_duration >= 30) {
                            if(e && e.vrp_keep_timer && timer) timer.stop();
                            // if(play_duration > video_length) play_duration = video_length;

	                        const formData = new FormData();
	                        formData.append("action", "insert_video_tracking");
	                        formData.append("post_id", post_id);
	                        formData.append("play_duration", play_duration);
	                        formData.append("video_attachment_type", getCookieValue('video_quality') || 'HD');

                                                            formData.append("video_action", 'play_paid_video');
                                formData.append("uid", '2091545');
                            
	                        navigator.sendBeacon("https://vrporn.com/video-tracking.php", formData);
	                    }
                    }

                    
