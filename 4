        function av_sfw_check_exclude_page() {
            const excludedPages = new Set([
                '/sfw-lander',
                '/dumpheaders',
                '/premiumlp',
                '/signup',
                '/members/',
                '/login',
                '/logout',
                '/terms-of-service',
                '/privacy-policy',
                '/dmcacopyright',
                '/ccpa',
                '/complaint-policy',
                '/affiliate-agreement',
                '/2257-statement',
                '/password-reset',
                '/age-verification',
                '/wp-login',
                '/wp-signup',
                '/tag/asian/',
                '/tag/threesome/',
                '/nats-hit.php',
                '/wp-admin/admin-ajax.php'
            ]);

            const currentUrlPath = window.location.pathname;

            return Array.from(excludedPages).some(page => currentUrlPath.startsWith(page));
        }

         function QuickgetCookie(name) {
            return document.cookie
                .split('; ')
                .find(row => row.startsWith(name + '='))
                ?.split('=')[1] || null;
        }
        let block_on_sfw = QuickgetCookie('x_amzn_waf_bs');
        if(block_on_sfw == 1 ){
            let vrp_user_is_logged_in = QuickgetCookie('vrp_user_is_logged_in');
            if (vrp_user_is_logged_in != 1 && !av_sfw_check_exclude_page()) {
                let urlParams = new URLSearchParams(window.location.search);
                let aParam = urlParams.get('a');
                let redirectTo = window.location.origin + '/blockstate.php';
                if (aParam) {
                    redirectTo = window.location.origin + '/blockstate.php?a=' + aParam;
                 }
                let date = new Date();
                date.setTime(date.getTime() + (30 * 24 * 60 * 60 * 1000));
                let expires = "expires=" + date.toUTCString();
                document.cookie = "block_on_sfw=1; path=/; " + expires;
                window.location.href = redirectTo;
            }
        } else {
             document.cookie = "block_on_sfw=; expires=Thu, 01 Jan 1970 00:00:00 UTC; path=/";
        }

