            function setPlayerCookie(c_name, value, exdays) {
                var exdate = new Date();
                exdate.setDate(exdate.getDate() + exdays);
                var c_value = escape(value) + ((exdays == null) ? "" : "; expires=" + exdate.toUTCString()) + ";domain=.vrporn.com;path=/";
                document.cookie = c_name + "=" + c_value;
            }

